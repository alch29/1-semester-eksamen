<div>
  {{#each stations}}
    <div class="stationCard box"
        data-name="{{this.name}}"
        data-address="{{this.address}}">
      <a href="/admin/stations/info/{{this.id}}">
        <img src="/img/gas-station-placeholder.avif" class="stationPlaceholderImg">
        <div class="stationCardOverlay">
            <div class="lowLineHeight centerText">
              <span class="subHeadline">{{this.name}}</span><br>               
              <span class="paragraph">{{this.address}}</span>
            </div>
