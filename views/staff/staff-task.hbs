<div>
    <button class="accordionHeader">
        <p class="subHeadline">Products</p>
        <i></i>
    </button>
    <div class="accordionContent">
        {{#each products}}
            <div class="accordionProduct"> 
                <p class="subHeadline">{{this.name}}</p>
                <input type="number" placeholder="Antal">
                <p class="paragraph">{{this.[measurement.measurement_symbol]}}</p>
            </div>
        {{/each}}
    </div>
</div>

<form action="/staff/stations" method="post" enctype="multipart/form-data">
   
    <div>
        <label for="image">Vaskehal</label>
        <input type="file" id="image-carwash" name="image" accept="image/*" multiple>
    </div>

    <div>
        <label for="image">Forplads</label>
        <input type="file" id="image-forcourt" name="image" accept="image/*" multiple>
    </div>

   <button type="submit" class="button2">Upload</button>

</form>